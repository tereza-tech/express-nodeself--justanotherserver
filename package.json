{
  "name": "myapp",
  "version": "1.0.0",
  "description": "My cool TypeScript app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "tsc",
    "dev": "nodemon src/server/index.ts",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" src/server/index.ts",
    "build:docker": "docker build -t pythagoras/express-ts .",
    "lint": "eslint -c .eslintrc.js \"{server}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server}/**/*.{js,ts,tsx}\" --quiet --fix"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.3",
    "express-openapi-validator": "^4.13.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
