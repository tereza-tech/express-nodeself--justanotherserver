    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./dist/",
    "rootDir": "./src",
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
